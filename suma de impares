#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"
using namespace std;


void main()
{
 int a,b,resto;
 cin>> a;
 cin>> b;
 if ((a%2!=0)&&(b%2!=0))
	  resto=(a+b);
 else 
	 resto=(a*b);
 coutt<< resto;
 getch();

}
